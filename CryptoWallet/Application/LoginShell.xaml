<?xml version="1.0" encoding="UTF-8"?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
       xmlns:onboarding="clr-namespace:CryptoWallet.Modules.Onboarding"
    xmlns:login="clr-namespace:CryptoWallet.Modules.Login"
    xmlns:register="clr-namespace:CryptoWallet.Modules.Register"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="CryptoWallet.LoginShell">
    <ShellContent Route="OnboardingViewModel"
                  ContentTemplate="{DataTemplate onboarding:Onboardingview}">
    </ShellContent>

    <ShellContent Route="LoginViewModel"
                  ContentTemplate="{DataTemplate login:LoginView}">
    </ShellContent>

    <ShellContent Route="RegisterViewModel"
                  ContentTemplate="{DataTemplate register:RegisterView}">
    </ShellContent>
</Shell>
